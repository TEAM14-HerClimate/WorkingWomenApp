<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - HerClimate</title>
 
</head>

<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>
       Working Women App
    </title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
   
    <link rel="stylesheet" href="~/WorkingWomenApp.styles.css" asp-append-version="true" />
    <link rel="shortcut icon"
          href="~/assets/images/favicon.png"
          type="image/x-icon" />
 @*    <link rel="stylesheet" href="~/css/assets/css/swiper-bundle.min.css" />
    <link rel="stylesheet" href="~/css/assets/css/animate.css" />
    <link rel="stylesheet" href="~/css/src/input.css" />
    <link rel="stylesheet" href="~/css/src/css/tailwind.css" /> *@
   @*  <link rel="stylesheet" href="~/css/style.css"> *@
    <link rel="stylesheet" href="~/css/site.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.2/css/all.min.css" />
    <link href='https://fonts.googleapis.com/css?family=Bitter' rel='stylesheet'>

</head>
<body>
    <!-- ====== Navbar Section Start -->
    <nav>
        <div class="menu">
        
            <ul>
                <div class="logo">
                    <a href="#">Her<span style="color:#49AA97;">Climate</span></a>
                </div>
                <li><a asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
                <li><a asp-area="" asp-controller="Home" asp-action="Dashboard">Dashboard</a></li>
                <li><a href="#">Services</a></li>
                <li><a href="#">Contact</a></li>
                <li><a asp-area="" asp-controller="Account" asp-action="Login">Login</a></li>
            </ul>
        </div>
    </nav>
    <!-- ====== Navbar Section End -->
    <div class="p-5 ">
      
            @RenderBody()
        
    </div>

    <!-- ====== Footer Section Start -->
    

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script>
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    </script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
